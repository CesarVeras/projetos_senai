package jogo;

import java.awt.Image;

public abstract class Inimigo extends ObjetoVivo{

	private int vidas;
	private int moving;

	public Inimigo(int posX, int posY, int width, int height, int velX,
			int velY, Image sprite, int frameX, int frameY, int colunas,
			int linhas, int vidas, int vidas2, int moving) {
		super(posX, posY, width, height, velX, velY, sprite, frameX, frameY,
				colunas, linhas, vidas);
		vidas = vidas2;
		this.moving = moving;
	}

	public void setMoving(int moving) {
		this.moving = moving;
	}

	public int getMoving() {
		return moving;
	}
}
