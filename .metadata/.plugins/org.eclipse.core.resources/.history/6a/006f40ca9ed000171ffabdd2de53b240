package jogo;

import java.awt.Color;
import java.awt.Graphics2D;
import java.awt.Image;

public class Chao extends ObjetoGrafico{
	
	public Chao(int posX, int posY, int width, int height, int velX, int velY,
			Image sprite) {
		super(posX, posY, width, height, velX, velY, sprite);
	}

	@Override
	public void draw(Graphics2D g) {
		if (getSprite() != null) {
			super.draw(g);
		} else {
			g.setColor(Color.green);
			g.fillRect(getPosX(), getPosY(), getWidth(), getHeight());
		}
		
	}

	@Override
	public void update() {
	}
}
